>R√©sultat :</h3>\n        <pre class=\"text-sm\">{{ JSON.stringify(result, null, 2) }}</pre>\n      </div>\n      \n      <div v-if=\"error\" class=\"mt-6 p-4 bg-red-100 border border-red-300 rounded-md\">\n        <h3 class=\"font-semibold text-red-800 mb-2\">Erreur :</h3>\n        <p class=\"text-red-700\">{{ error }}</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nconst email = ref('sophie.martin@activibe.com')\nconst password = ref('password')\nconst loading = ref(false)\nconst result = ref(null)\nconst error = ref('')\n\nconst testLogin = async () => {\n  loading.value = true\n  result.value = null\n  error.value = ''\n  \n  try {\n    const { $api } = useNuxtApp()\n    console.log('üîë Test de connexion avec:', { email: email.value, password: '***' })\n    console.log('üîë URL de base API:', useRuntimeConfig().public.apiBase)\n    \n    const response = await $api.post('/auth/login', {\n      email: email.value,\n      password: password.value\n    })\n    \n    result.value = {\n      success: true,\n      message: 'Connexion r√©ussie !',\n      user: response.data.user,\n      token: response.data.token?.substring(0, 20) + '...'\n    }\n    \n    console.log('‚úÖ Connexion r√©ussie:', response.data)\n  } catch (err) {\n    console.error('‚ùå Erreur de connexion:', err)\n    \n    error.value = `Erreur: ${err.message}`\n    if (err.response?.data?.message) {\n      error.value = err.response.data.message\n    }\n    if (err.response?.status) {\n      error.value += ` (Status: ${err.response.status})`\n    }\n    \n    result.value = {\n      success: false,\n      error: error.value,\n      details: err.response?.data\n    }\n  } finally {\n    loading.value = false\n  }\n}\n</script>\n"],"version":3}